#include <bits/stdc++.h>
#define int long long
using namespace std;
int a, b, c, d;

 int max_of_four(int a, int b, int c, int d)
 {
     int MAX1 = 0;
     int MAX2 = 0;
     MAX1 = max(a,b);
     MAX2 = max(c,d);
     return max(MAX1,MAX2);
 }

int32_t main()
{
    cin >> a;
    cin >> b;
    cin >> c;
    cin >> d;
    cout << max_of_four(a,b,c,d);
}
