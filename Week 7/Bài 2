#include <bits/stdc++.h>
using namespace std;

int main()
{
    char letters[] = {'a', 'b', 'c', 'd'};
    int n = sizeof(letters) / sizeof(letters[0]);

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            for (int k = 1; k <= n; k++) {
                cout << letters[i] << letters[j] << letters[k] << endl;
            }
        }
    }

    return 0;
}
