#include <bits/stdc++.h>
using namespace std;

int BinarySearch(int a[], int left, int right, int num)
{
    while ( left <= right )
    {
        int middle = (left + right) / 2;
        if (a[middle] == num)
        {
            return middle;
        }
        if (a[middle] < num)
        {
            left = middle + 1;
        }
        else
        {
            right = middle + 1;
        }
    }
    return -1;
}
int n, a[1000];
int main()
{
    cin >> n;
    for (int i = 1; i <= n; i++)
    {
        cin >> a[i];
    }
    int num;
    cin >> num;
    int result = BinarySearch(a , 1 , n , num);
    if (result == -1)
    {
        cout << "Number not found";
    }
    else
    {
        cout << result;
    }
    return 0;
}

